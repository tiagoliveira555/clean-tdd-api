    "module-alias": "^2.2.2",
    "mongo-round": "^1.0.0",
    "mongodb": "^4.1.1",
    "rimraf": "^4.1.2",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@shelf/jest-mongodb": "^2.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/module-alias": "^2.0.1",
    "@types/mongodb": "^4.0.7",
